{
  "autoload": {
    "psr-4": {
      "TFD\\Cloudinary\\": "src"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "TFD\\Cloudinary\\ServiceProvider"
      ]
    },
    "statamic": {
      "description": "Cloudinary addon",
      "name": "Cloudinary"
    }
  },
  "license": "MIT",
  "name": "tfd/statamic-cloudinary",
  "require": {
    "statamic/cms": "3.1.* || 3.2.* || 3.3.* || 3.4.*"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2",
    "phpunit/phpunit": "^9.3",
    "squizlabs/php_codesniffer": "^3.6"
  },
  "scripts": {
    "lint": "@php ./vendor/bin/php-cs-fixer fix -v --config .php-cs-fixer.php"
  }
}
